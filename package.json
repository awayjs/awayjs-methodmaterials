    "@awayjs/core": "^0.6.0",
    "@awayjs/display": "^0.8.0",
    "@awayjs/stage": "^0.8.0",
    "@awayjs/renderer": "^0.8.0"
  },
  "devDependencies": {
    "rimraf": "^2.5.2",
    "typescript": "^1.9.0-dev.20160603-1.0"
